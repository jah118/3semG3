<Page x:Class="RestaurantDesktopClient.ManagePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RestaurantDesktopClient"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ManagePage" MinHeight="500" MinWidth="300">
    

    <Grid x:Name="LayoutRoot" >
       

        <StackPanel Grid.RowSpan="3" Grid.ColumnSpan="2" VerticalAlignment="Top">
            <Frame Source="HeadLine.xaml" Grid.ColumnSpan="2" />
            <DockPanel Grid.Row="1" Grid.ColumnSpan="2" HorizontalAlignment="Stretch" >
                <Frame Source="Listpage.xaml" Margin="15,0,0,0" />
                <Frame Source="CUDEmployee.xaml" />
            </DockPanel>
            <Frame Source="Back.xaml"/>
        </StackPanel>


    </Grid>
</Page>
